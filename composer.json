{
    "name": "stanislav-web/phalcon-development",
    "description": "Phalcon Development Board",
    "type": "project",
    "keywords": [
        "framework",
        "phalcon",
        "phalcon rest",
        "phalcon development",
        "phalcon board",
        "phalcon admin tools",
        "phalcon administrator",
        "phalcon dashboard"
    ],
    "homepage": "http://reds.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Stanislav WEB",
            "email": "stanisov@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
      "psr-4": {
        "Application\\": "Application/"
      }
    },
  "prefer-stable": true,
  "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "ext-phalcon": ">=2.0",
        "ext-apcu": ">=4.0",
        "ext-mcrypt" : "*",
        "ext-igbinary" : "*",
        "stanislav-web/phalcon-sms-factory": "dev-master",
        "stanislav-web/phalcon-translate": "1.3-stable",
        "stanislav-web/phalcon-uploader": "dev-master",
        "swiftmailer/swiftmailer": "5.*",
        "phalcon/incubator": "2.*",
        "stanislav-web/httpstatuses-json": "1.0-stable"
    },
    "require-dev": {
        "robmorgan/phinx": "dev-master",
        "codeception/codeception" :"*",
        "bruli/php-git-hooks": "~1.0@dev",
        "phalcon/devtools": "dev-master",
        "flow/jsonpath": "dev-master"
    },
    "config": {
        "bin-dir": "./vendor/bin"
    }
}
